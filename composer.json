{
  "name": "avionicsbird/laravel-filepond",
  "description": "Laravel backend module for filepond uploads",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "filepond",
    "upload",
    "image"
  ],
  "authors": [
    {"name" : "Rick Bird",
     "email" : "support@cryptom.city"
    },
    {
      "name": "Paul Mohr",
      "email": "p.mohr@sopamo.de"
    }
  ],
  "require": {
    "php": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Sopamo\\LaravelFilepond\\": "src/"
    }
  },
  "extra": {
